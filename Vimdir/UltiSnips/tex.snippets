snippet mindmap "Tikz Mind Map" b
\begin{tikzpicture}
    \begin{scope}[mindmap,concept color=black,text=white,
        every node/.style={concept,circular drop shadow},
        root/.style={font=\normalsize\bf,text width=7.7em,circular drop shadow},
        level 1/.append style={font=\small\bf,sibling angle=40},
        level 2/.append style={font=\footnotesize\bf,level distance=3cm,sibling angle=40},
        level 3/.append style={font=\scriptsize\bf,level distance=2cm,sibling angle=40},
        level 4/.append style={font=\tiny\bf,level distance=1cm,sibling angle=40}]
        \node[root] {$0};
    \end{scope}
    \begin{scope}[
        every annotation/.style = {draw,fill=white,font=\normalsize,text=black,drop shadow}]
    \end{scope}
\end{tikzpicture}
endsnippet

